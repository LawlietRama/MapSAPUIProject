<mvc:View controllerName="btp.project1.controller.App"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <App id="app">
        <Page id="page" title="{i18n>title}">
            <content>
                <Button xmlns="sap.m" text="Button" id="button" press="onClick"/>
                <Input value="{/recipient/name}" description="Hello {/recipient/name}" valueLiveUpdate="true" width="60%"/>
                <Text text="Tes {/recipient/name}" />
            </content>
        </Page>
    </App>
</mvc:View>
